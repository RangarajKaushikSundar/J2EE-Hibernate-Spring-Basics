<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="diemp" class="com.cg.di.Employee">
		<constructor-arg value="Krish"  index="1"></constructor-arg>
		<constructor-arg value="Manager" index="2"></constructor-arg>
		<constructor-arg value="56000" index="3"></constructor-arg>
		<constructor-arg value="1001" index="0"></constructor-arg>
		<constructor-arg >
				<bean class="com.cg.di.Project">
				<property name="pname" value="MyProject"></property>
		</bean>
		
		</constructor-arg>
	</bean>
	<bean id="beanlife" class="com.jsl.spring.ExampleBean" init-method="afterPropertiesSet" destroy-method="destroy">
		
	</bean>
</beans>
